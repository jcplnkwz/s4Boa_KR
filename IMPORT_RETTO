			ifelse(vate$yT=="4c", "4c",
			ifelse(vate$yT=="T3", "3",
			ifelse(vate$yT=="x", "X",
			ifelse(vate$yT=="X", "X",
					 NA  ))))))))))))))))
					 
vate$yN <-  ifelse(vate$yN=="0", "0",
			ifelse(vate$yN=="1", "1",
