vate$yM <-  ifelse(vate$yM=="  0", "0",
			ifelse(vate$yM=="0", "0",
			ifelse(vate$yM=="1", "1",
			ifelse(vate$yM=="2", "2",
			ifelse(vate$yM=="x", "X",
