					 NA  ))))))
					 
p_staging_ajcc_scale <-  ifelse(vate$P_Staging_Date <= "2002-12-31" , "AJCC 5th",
			ifelse(vate$P_Staging_Date > "2002-12-31" & vate$P_Staging_Date <= "2010-12-31", "AJCC 6th",
